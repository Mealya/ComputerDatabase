<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">
	<servlet>
		<servlet-name>Dashboard</servlet-name>
		<servlet-class>com.excilys.controller.Dashboard</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddComputer</servlet-name>
		<servlet-class>com.excilys.controller.AddComputer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditComputer</servlet-name>
		<servlet-class>com.excilys.controller.EditComputer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteComputer</servlet-name>
		<servlet-class>com.excilys.controller.DeleteComputer</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Dashboard</servlet-name>
		<url-pattern>/dash</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddComputer</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditComputer</servlet-name>
		<url-pattern>/edit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteComputer</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>

